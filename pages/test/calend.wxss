.calend-test {
  background: #F8F8FA;
  height: 100vh;
  overflow-y: scroll;
}
.calend-test .header {
  height: 88rpx;
  background-color: #fff;
  margin-bottom: 24rpx;
}

.booking-info {
  margin-bottom: 20rpx;
}
.booking-info .address-and-date {
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ECEEEF;
}
.booking-info .address-and-date .address-icon {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
  background-size: 100%;
  background-image: url("data:image/png;base64,");
}

.booking-info .address-and-date .main {
  flex: 1;
}

.booking-info .address-and-date .main .address {
  font-size: 36rpx;
  font-weight: 500;
  color: #2B2827;
  line-height: 40rpx;
  padding-bottom: 12rpx;
}

.booking-info .address-and-date .main .date-box {
  display: flex;
  color: #83817F;
  font-size: 28rpx;
  padding-bottom: 24rpx;
}

.booking-info .address-and-date .main .date-box .date {
  color: #FF632B;
  padding: 0 20rpx;
  font-size: 500;
}

.booking-info .address-and-date .main .date-box .time {
  color: #FF632B;
  font-size: 500;
}

.booking-info  .check-item {
  padding-top: 22rpx;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  align-items: center;
  color: #494847;
  justify-content: space-between;
  font-size: 28rpx;
}

.booking-info  .check-item .right-icon {
  width: 32rpx;
  height: 32rpx;
  background-size: 100%;
  color: #494847;
  background-image: url('data:image/png;base64,');
}